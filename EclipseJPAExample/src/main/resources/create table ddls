create table CNTC_RLTNSHP(
CNTC_RLTNSHP_ID INTEGER NOT NULL PRIMARY KEY,
O_WRK_ID BIGINT NOT NULL,
CNTV_TYP_TXT VARCHAR(35) NOT NULL,
RLTNSHP_TYP_TXT VARCHAR(35) NOT NULL,
CNTC_FRST_NM VARCHAR(30) NOT NULL,
CNTC_MID_NM VARCHAR(30) NOT NULL,
CNTC_LST_NM VARCHAR(30) NOT NULL,
PFRD_NM VARCHAR(50) NULL
)

DROP TABLE CNTC_RLTNSHP

CREATE TABLE CNTC_RLTNSHP_PH(
CNTC_RLTNSHP_ID INTEGER NOT NULL,
FOREIGN KEY (CNTC_RLTNSHP_ID)
REFERENCES CNTC_RLTNSHP(CNTC_RLTNSHP_ID)
ON UPDATE CASCADE ON DELETE CASCADE,
PH_TYP_TXT VARCHAR(35) NOT NULL,
PRIMARY KEY(CNTC_RLTNSHP_ID,PH_TYP_TXT),
CNTC_PH_NMBR_TXT VARCHAR(24) NOT NULL
)

CREATE TABLE CNTC_RLTNSHP_EML(
CNTC_RLTNSHP_ID INT NOT NULL,
FOREIGN KEY (CNTC_RLTNSHP_ID)
REFERENCES CNTC_RLTNSHP(CNTC_RLTNSHP_ID)
ON UPDATE CASCADE ON DELETE CASCADE,
EML_TYP_TXT VARCHAR(35) NOT NULL,
PRIMARY KEY(CNTC_RLTNSHP_ID,EML_TYP_TXT),
EML_ADDR_TXT VARCHAR(70) NOT NULL
)

CREATE TABLE CNTC_RLTNSHP_ADDR(
CNTC_RLTNSHP_ID INT NOT NULL,
FOREIGN KEY (CNTC_RLTNSHP_ID)
REFERENCES CNTC_RLTNSHP(CNTC_RLTNSHP_ID)
ON UPDATE CASCADE ON DELETE CASCADE,
ADDR_TYP_TXT VARCHAR(35) NOT NULL,
PRIMARY KEY(CNTC_RLTNSHP_ID,ADDR_TYP_TXT),
CNTC_ADDR_LNE_1 VARCHAR(35) NOT NULL,
CNTC_ADDR_LNE_2 VARCHAR(35) NULL,
CNTC_ADDR_LNE_3 VARCHAR(35) NULL,
CNTC_ADDR_LNE_4 VARCHAR(35) NULL,
CNTC_CTY_NM VARCHAR(20) NOT NULL,
CNTC_ST_CD VARCHAR(6) NULL,
CNTC_ST_NM VARCHAR(20) NULL,
CNTC_CNTY VARCHAR(30) NULL,
CNTC_CNTRY VARCHAR(30) NOT NULL,
CNTC_PSTL_CD CHAR(10) NOT NULL
)

CREATE TABLE DEMOGRAPHIC(
O_WRK_ID INT NOT NULL,
